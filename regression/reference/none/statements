c_function_shadow&_capptr:
  name: c_function_shadow&_capptr
  comments:
  - Return a C_capsule_data_type.
  intent: function
  i_arg_names:
  - '{i_var}'
  i_arg_decl:
  - 'type({f_capsule_data_type}){f_intent_attr} :: {i_var}'
  i_result_decl:
  - 'type(C_PTR) :: {i_result_ptr}'
  i_result_var: '{i_result_ptr}'
  i_module:
    iso_c_binding:
    - C_PTR
    '{f_module_name}':
    - '{f_capsule_data_type}'
  c_arg_decl:
  - '{c_type} * {c_var}'
  c_call:
  - '{c_const}{cxx_type} &{c_local_cxx} = {C_call_function};'
  c_post_call:
  - '{c_var}->addr = {cxx_nonconst_ptr};'
  - '{c_var}->idtor = {idtor};'
  c_return:
  - return {c_var};
  c_return_type: '{c_type} *'
  c_local:
  - cxx
  owner: library
