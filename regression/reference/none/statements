c_inout_string&:
  name: c_inout_string&
  comments:
  - Pass CHARACTER and LEN to C wrapper.
  intent: inout
  i_arg_names:
  - '{i_var}'
  i_arg_decl:
  - 'character(kind=C_CHAR){f_intent_attr} :: {i_var}(*)'
  i_module:
    iso_c_binding:
    - C_CHAR
  c_arg_decl:
  - '{gen.cdecl.c_var}'
  c_arg_call:
  - '{c_local_cxx}'
  c_pre_call:
  - '{c_const}std::string {c_local_cxx}({c_var});'
  c_post_call:
  - strcpy({c_var}, {c_local_cxx}.c_str());
  c_local:
  - cxx
  impl_header:
  - <cstring>
  owner: library
  lang_cxx:
    impl_header:
    - <cstring>
