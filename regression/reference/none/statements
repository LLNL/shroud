  - '{gen.cidecl.c_var}'
  c_arg_call:
  - '{cxx_var}'
  c_pre_call:
  - "{gen.cxxdecl.cxx_var} =\t {gen.c_to_cxx};"
  fmtdict:
    cxx_var: '{CXX_local}{c_var}'
  owner: library
