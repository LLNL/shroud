        scalar -- py_in_shadow_scalar
      string
        & -- py_in_string_&
        * -- py_in_string_*
        scalar -- py_in_string_scalar
      struct
        &
          class -- py_in_struct_&_class
          numpy -- py_in_struct_&_numpy
        *
          class -- py_in_struct_*_class
          list -- py_in_struct_*_list
          numpy -- py_in_struct_*_numpy
        list -- py_in_struct_list
