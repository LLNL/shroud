  owner: library
  post_call:
  - if ({cxx_var} != {nullptr}) {{+
  - "{c_temp_lower_decl}{c_temp_extents_decl}int SH_ret = CFI_allocate({c_var_cfi},\
    \ \t{c_temp_lower_use}, \t{c_temp_extents_use}, \t0);"
  - if (SH_ret == CFI_SUCCESS) {{+
