  f_declare:
  - integer(C_INT) {f_var_len}
  f_pre_call:
  - '{f_var_len} = len({f_var}, kind=C_INT)'
  f_arg_call:
  - '{f_var}'
  - '{f_var_len}'
  f_module:
    '{f_module_name}':
    - '{f_kind}'
    iso_c_binding:
    - C_INT
  f_temps:
  - len
  f_need_wrapper: true
  i_arg_names:
  - '{i_var}'
  - '{i_var_len}'
  i_arg_decl:
