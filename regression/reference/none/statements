  - '{c_const}char *{c_local_cxx} = {C_call_function};'
  c_post_call:
  - "{c_helper_char_copy}({c_var}, {c_var_len},\t {c_local_cxx},\t -1);"
  c_return_type: void
  c_temps:
  - len
  c_local:
  - cxx
  c_helper:
  - char_copy
  owner: library
