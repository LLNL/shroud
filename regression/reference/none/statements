  name: py_in_native*_numpy
  intent: in
  arg_call:
  - '{c_var}'
  need_numpy: true
  parse_format: O
  parse_args:
  - '&{pytmp_var}'
  declare:
  - PyObject * {pytmp_var};
  - PyArrayObject * {py_var} = {nullptr};
  post_parse:
