  - '{c_local_cxx}'
  c_pre_call:
  - std::string {c_local_cxx};
  c_post_call:
  - "{c_helper_char_copy}({c_var}, {c_var_len},\t {c_local_cxx}.data(),\t {c_local_cxx}.size());"
  c_temps:
  - len
  c_local:
  - cxx
  c_helper:
  - char_copy
  owner: library
