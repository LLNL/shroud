c_out_vector_buf_targ_string_scalar:
  c_arg_decl:
  - const char *{c_var}
  - size_t {c_var_size}
  - int {c_var_len}
  c_helper: ShroudLenTrim
  cxx_local_var: scalar
  f_arg_decl:
  - 'character(kind=C_CHAR), intent(IN) :: {c_var}(*)'
  - 'integer(C_SIZE_T), intent(IN), value :: {c_var_size}'
  - 'integer(C_INT), intent(IN), value :: {c_var_len}'
  f_c_arg_names:
  - '{c_var}'
  - '{c_var_size}'
  - '{c_var_len}'
  f_module_line: iso_c_binding:C_CHAR,C_SIZE_T,C_INT
  intent: out
  local:
  - i
  - n
  - s
  name: c_out_vector_buf_targ_string_scalar
  owner: library
  post_call:
  - '{{+'
  - char * {c_local_s} = {c_var};
  - std::vector<{cxx_T}>::size_type
  - +{c_local_i} = 0,
  - '{c_local_n} = {c_var_size};'
  - '{c_local_n} = std::min({cxx_var}.size(),{c_local_n});'
  - -for(; {c_local_i} < {c_local_n}; {c_local_i}++) {{+
  - "ShroudStrCopy({c_local_s}, {c_var_len},\t {cxx_var}[{c_local_i}].data(),\t {cxx_var}[{c_local_i}].size());"
  - '{c_local_s} += {c_var_len};'
  - -}}
  - -}}
  pre_call:
  - '{c_const}std::vector<{cxx_T}> {cxx_var};'
  temps:
  - size
  - len
c_out_vector_cdesc_targ_native_scalar:
  c_arg_decl:
  - '{C_array_type} *{c_var_cdesc}'
  c_helper: ShroudTypeDefines
  cxx_local_var: pointer
  destructor_name: std_vector_{cxx_T}
  f_arg_decl:
