  i_import:
  - '{F_array_type}'
  c_arg_decl:
  - '{C_array_type} *{c_var_cdesc}'
  c_arg_call:
  - '{cxx_var}'
  c_pre_call:
  - "{cxx_type} * {c_var} = static_cast<{cxx_type} *>\t(const_cast<void *>({c_var_cdesc}->base_addr));"
  c_temps:
  - cdesc
  c_helper:
  - array_context
  owner: library
  lang_c:
    c_pre_call:
    - '{cxx_type} * {c_var} = {c_var_cdesc}->base_addr;'
  lang_cxx:
    c_pre_call:
    - "{cxx_type} * {c_var} = static_cast<{cxx_type} *>\t(const_cast<void *>({c_var_cdesc}->base_addr));"
