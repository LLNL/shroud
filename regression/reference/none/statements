c_ctor_shadow_scalar_capptr:
  c_arg_decl:
  - '{c_type} * {c_var}'
  call:
  - "{cxx_type} *{cxx_var} =\t new {cxx_type}({C_call_list});"
  - "{c_var}->addr = static_cast<{c_const}void *>(\t{cxx_var});"
  - '{c_var}->idtor = {idtor};'
  cxx_local_var: pointer
  f_arg_decl:
  - 'type({f_capsule_data_type}), intent({f_intent}) :: {c_var}'
  f_c_arg_names:
  - '{c_var}'
  f_module:
    iso_c_binding:
    - C_PTR
  f_module_line: '{f_c_module_line}'
  f_result_decl:
  - type(C_PTR) {F_result_ptr}
  f_result_var: '{F_result_ptr}'
  intent: ctor
  name: c_ctor_shadow_scalar_capptr
  owner: caller
  ret:
  - return {c_var};
c_ctor_shadow_scalar_capsule:
  c_arg_decl:
  - '{c_type} * {c_var}'
  call:
  - "{cxx_type} *{cxx_var} =\t new {cxx_type}({C_call_list});"
  - "{c_var}->addr = static_cast<{c_const}void *>(\t{cxx_var});"
  - '{c_var}->idtor = {idtor};'
  cxx_local_var: pointer
  f_arg_decl:
  - 'type({f_capsule_data_type}), intent({f_intent}) :: {c_var}'
  f_c_arg_names:
  - '{c_var}'
  f_module_line: '{f_c_module_line}'
  intent: ctor
  name: c_ctor_shadow_scalar_capsule
  owner: caller
  return_type: void
c_dtor:
  call:
  - delete {CXX_this};
  - '{C_this}->addr = {nullptr};'
  impl_header:
  - <cstddef>
  intent: dtor
  name: c_dtor
  owner: library
  return_type: void
c_function:
  intent: function
  name: c_function
  owner: library
c_function_char_*:
  f_module:
    iso_c_binding:
    - C_PTR
  f_result_decl:
  - type(C_PTR) {c_var}
  intent: function
  name: c_function_char_*
  owner: library
c_function_char_*_buf:
  c_arg_decl:
  - char *{c_var}
  - int {c_var_len}
  c_helper: ShroudStrCopy
  cxx_local_var: result
  f_arg_decl:
  - 'character(kind=C_CHAR), intent({f_intent}) :: {c_var}(*)'
  - 'integer(C_INT), value, intent(IN) :: {c_var_len}'
  f_c_arg_names:
  - '{c_var}'
  - '{c_var_len}'
  f_module:
    iso_c_binding:
    - C_CHAR
    - C_INT
  intent: function
  name: c_function_char_*_buf
  owner: library
  post_call:
  - "ShroudStrCopy({c_var}, {c_var_len},\t {cxx_var},\t -1);"
  return_type: void
  temps:
  - len
c_function_char_*_cdesc:
  c_arg_decl:
  - '{C_array_type} *{c_var_cdesc}'
  c_helper: ShroudTypeDefines
  f_arg_decl:
  - 'type({F_array_type}), intent(OUT) :: {c_var}'
  f_c_arg_names:
  - '{c_var}'
  f_import:
  - '{F_array_type}'
  intent: function
  name: c_function_char_*_cdesc
  owner: library
  post_call:
  - '{c_var_cdesc}->cxx.addr = {cxx_nonconst_ptr};'
  - '{c_var_cdesc}->cxx.idtor = {idtor};'
  - '{c_var_cdesc}->addr.ccharp = {cxx_var};'
  - '{c_var_cdesc}->type = {sh_type};'
  - '{c_var_cdesc}->elem_len = {cxx_var} == {nullptr} ? 0 : {stdlib}strlen({cxx_var});'
  - '{c_var_cdesc}->size = 1;'
  - '{c_var_cdesc}->rank = 0;'
  return_type: void
  temps:
  - cdesc
c_function_char_*_cfi:
  c_arg_decl:
  - CFI_cdesc_t *{c_var_cfi}
  c_helper: ShroudStrCopy
  cxx_local_var: result
  f_arg_decl:
  - 'character(len=*), intent({f_intent}) :: {c_var}'
  f_c_arg_names:
  - '{c_var}'
  iface_header:
  - ISO_Fortran_binding.h
  intent: function
  name: c_function_char_*_cfi
  owner: library
  post_call:
  - char *{c_var} = {cast_static}char *{cast1}{c_var_cfi}->base_addr{cast2};
  - "ShroudStrCopy({c_var}, {c_var_cfi}->elem_len,\t {cxx_var},\t -1);"
  return_type: void
  temps:
  - cfi
c_function_char_*_cfi_allocatable:
  c_arg_decl:
  - CFI_cdesc_t *{c_var_cfi}
  f_arg_decl:
  - 'character(len=:), intent({f_intent}), allocatable :: {c_var}'
  f_c_arg_names:
  - '{c_var}'
  iface_header:
  - ISO_Fortran_binding.h
  intent: function
  name: c_function_char_*_cfi_allocatable
  owner: library
  post_call:
  - if ({cxx_var} != {nullptr}) {{+
  - "int SH_ret = CFI_allocate({c_var_cfi}, \t(CFI_index_t *) 0, \t(CFI_index_t *)\
    \ 0, \tstrlen({cxx_var}));"
  - if (SH_ret == CFI_SUCCESS) {{+
  - "{stdlib}memcpy({c_var_cfi}->base_addr, \t{cxx_var}, \t{c_var_cfi}->elem_len);"
  - -}}
  - -}}
  return_type: void
  temps:
  - cfi
c_function_char_*_cfi_pointer:
  c_arg_decl:
  - CFI_cdesc_t *{c_var_cfi}
  f_arg_decl:
  - 'character(len=:), intent({f_intent}), pointer :: {c_var}'
  f_c_arg_names:
  - '{c_var}'
  iface_header:
  - ISO_Fortran_binding.h
  intent: function
  local:
  - cptr
  - fptr
  - cdesc
  - len
  - err
  name: c_function_char_*_cfi_pointer
  owner: library
  post_call:
  - int {c_local_err};
  - if ({cxx_var} == {nullptr}) {{+
  - "{c_local_err} = CFI_setpointer(\t{c_var_cfi},\t {nullptr},\t {nullptr});"
  - -}} else {{+
  - CFI_CDESC_T(0) {c_local_fptr};
  - CFI_cdesc_t *{c_local_cdesc} = {cast_reinterpret}CFI_cdesc_t *{cast1}&{c_local_fptr}{cast2};
  - void *{c_local_cptr} = {cxx_nonconst_ptr};
  - size_t {c_local_len} = {stdlib}strlen({cxx_var});
  - "{c_local_err} = CFI_establish({c_local_cdesc},\t {c_local_cptr},\t CFI_attribute_pointer,\t\
    \ CFI_type_char,\t {c_local_len},\t 0,\t {nullptr});"
  - if ({c_local_err} == CFI_SUCCESS) {{+
  - '{c_var_cfi}->elem_len = {c_local_cdesc}->elem_len;'
  - "{c_local_err} = CFI_setpointer(\t{c_var_cfi},\t {c_local_cdesc},\t {nullptr});"
  - -}}
  - -}}
  return_type: void
  temps:
  - cfi
c_function_char_scalar:
  c_arg_decl:
  - char *{c_var}
  call:
  - '*{c_var} = {function_name}({C_call_list});'
  f_arg_decl:
  - 'character(kind=C_CHAR), intent(OUT) :: {c_var}'
  f_c_arg_names:
  - '{c_var}'
  f_module:
    iso_c_binding:
    - C_CHAR
  intent: function
  name: c_function_char_scalar
  owner: library
  return_type: void
c_function_char_scalar_cfi:
  c_arg_decl:
  - CFI_cdesc_t *{c_var_cfi}
  f_arg_decl:
  - 'character(len=*), intent({f_intent}) :: {c_var}'
  f_c_arg_names:
  - '{c_var}'
  iface_header:
  - ISO_Fortran_binding.h
  impl_header:
  - <cstring>
  intent: function
  name: c_function_char_scalar_cfi
  owner: library
  post_call:
  - char *{c_var} = {cast_static}char *{cast1}{c_var_cfi}->base_addr{cast2};
  - '{stdlib}memset({c_var}, '' '', {c_var_cfi}->elem_len);'
  - '{c_var}[0] = {cxx_var};'
  temps:
  - cfi
