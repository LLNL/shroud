  c_post_call:
  - char *{c_var} = {cast_static}char *{cast1}{c_var_cfi}->base_addr{cast2};
  - if ({c_local_cxx}{cxx_member}empty()) {{+
  - "{c_helper_char_copy}({c_var}, {c_var_cfi}->elem_len,\t {nullptr},\t 0);"
  - -}} else {{+
